{
  "name": "dabulang",
  "version": "1.0.0",
  "description": "A simple imperative programming language with a BASIC-inspired syntax",
  "configuration": {
    "type": "object",
    "title": "Example configuration",
    "properties": {
      "languageServerExample.maxNumberOfProblems": {
        "scope": "resource",
        "type": "number",
        "default": 100,
        "description": "Controls the maximum number of problems produced by the server."
      }
    }
  },
  "main": "vscode/out/main.js",
  "scripts": {
    "postinstall": "cd vscode && npm i",
    "compile": "tsc -b",
    "watch": "tsc -b -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stickyPiston/dabulang.git"
  },
  "engines": {
    "vscode": "^1.63.0"
  },
  "activationEvents": [
    "onLanguage:plaintext"
  ],
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/stickyPiston/dabulang/issues"
  },
  "homepage": "https://github.com/stickyPiston/dabulang#readme",
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/node": "^16.11.7",
    "@types/vscode": "^1.63.0",
    "@vscode/test-electron": "^2.1.2",
    "typescript": "^4.7.2"
  },
  "dependencies": {
    "glob": "^8.0.3"
  }
}
